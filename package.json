{
  "name": "js-docker-redis",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run prisma && concurrently \"npm run start:server\" \"npm run start:worker\"",
    "start:server": "npx ts-node server.ts",
    "start:worker": "npx ts-node workers/emailVerification.ts",
    "prisma": "npx prisma introspect && npx prisma generate"
  },
  "devDependencies": {
    "@prisma/cli": "^2.2.0",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.18",
    "@types/redis": "^2.8.24",
    "concurrently": "^5.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@prisma/client": "^2.2.0",
    "express": "^4.17.1",
    "redis": "^3.0.2"
  }
}
